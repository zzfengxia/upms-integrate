<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zz.upms.base.dao.system.DictDao">
    <select id="findDictItem" parameterType="java.lang.String" resultType="com.zz.upms.base.domain.system.Dict">
        select * from tm_dict where dict_type = #{dictType} and dict_key = #{dictKey}
    </select>

    <select id="findDictItems" parameterType="java.lang.String" resultType="com.zz.upms.base.domain.system.Dict">
        select * from tm_dict where dict_hash = #{dictType} order by dict_ord
    </select>
</mapper>